{
  "name": "contributte/fx-skeleton",
  "description": "All time best project skeleton for Nette Framework (@nette). Bootloader, DDD, TDD, Command Bus, Event Bus, Message Bus, API, Middlewares, ORM, DBAL. Using Contributte (@contributte) libraries by @f3l1x.",
  "keywords": [
    "nette",
    "project",
    "ddd",
    "tdd",
    "bus",
    "cqrs",
    "middleware",
    "orm",
    "dbal"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=8.2",
    "contributte/kernel": "^0.1.0",
    "contributte/framex": "^0.1.0",
    "contributte/utils": "^0.6.0"
  },
  "require-dev": {
    "contributte/qa": "^0.3",
    "contributte/dev": "^0.4",
    "contributte/phpstan": "^0.1",
    "contributte/tester": "^0.2"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
